local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/memejames/elerium-v2-ui-library//main/Library", true))()
local window = library:AddWindow("Spectra Test", {
	main_color = Color3.fromRGB(41, 74, 122), -- Color
	min_size = Vector2.new(250, 346), -- Size of the gui
	can_resize = false, -- true or false
})

local Pets = window:AddTab("Pets") -- Name of tab
Pets:Show() -- shows the tab

Pets:AddLabel("=== Buy Pets ===")

local dropdownpets = Pets:AddDropdown("Select Pets", function(text)
	if text == "Mars" then  -- Code
		print("o")
	elseif text == "Earth" then
	print("k")
	elseif text == "Iridocyclitis" then
	print("Weeeee")
	end
end)

local switchpet = Pets:AddSwitch("Buy Pets", function(bool)
	 -- toggle_god_mode(bool)
end)
switchpet:Set(false)

